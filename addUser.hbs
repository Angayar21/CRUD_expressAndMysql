<h3>Add New Student</h3>
{{#if msg}}
<div class="alert">{{msg}}

</div>{{/if}}
<form method="post" action="/addUser">
<div class="form-group">
    <label for="name">Name</label>
    <input type="text" name="name">
</div>
<div class="form-group">
    <label for="Age">Age</label>
    <input type="text" name="age">
</div>
